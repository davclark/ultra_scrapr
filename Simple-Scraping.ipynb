     "source": [
      "This is the core ultra_scrapr library (have a look at the library yourself!)"
     ]
    },
    {
     "cell_type": "code",
     "collapsed": false,
     "input": [
      "import requests\n",
      "import io\n",
      "\n",
      "def save_web_to_file(url):\n",
      "    '''Use requests to fetch url, then save to a file'''\n",
      "    r = requests.get(url)\n",
      "    # Might be a problem if URL doesn't start with http://\n",
      "    stripped_url = url.split('//')[1]\n",
      "    filename = stripped_url.replace('/', '_')\n",
      "    with io.open(filename, 'w', encoding='utf8') as outfile:\n",
      "        outfile.write(r.text)"
     ],
     "language": "python",
     "metadata": {},
     "outputs": [],
     "prompt_number": 2
    },
    {
